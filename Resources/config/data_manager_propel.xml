<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <!-- Field Guessers -->
        <parameter key="white_october_admin.field_guesser.propel.class">WhiteOctober\AdminBundle\DataManager\Propel\Guesser\PropelFieldGuesser</parameter>
    </parameters>

    <services>
        <!-- Field Guessers -->
        <service id="white_october_admin.field_guesser.propel" class="%white_october_admin.field_guesser.propel.class%">
            <tag name="white_october_admin.field_guesser" alias="propel" />
        </service>

        <!-- Actions -->
        <service id="white_october_admin.action.propel.crud" class="WhiteOctober\AdminBundle\DataManager\Propel\Action\CRUDActionCollection" public="false">
            <tag name="white_october_admin.action" alias="propel.crud" />
        </service>
    </services>

</container>
